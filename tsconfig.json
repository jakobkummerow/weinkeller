{
  "compilerOptions": {
      "outDir": "./js",
      "alwaysStrict": true,
      "module": "None",
      "moduleResolution": "Node",
      "noImplicitAny": true,
      "noImplicitThis": true,
      "sourceMap": true,
      "strictNullChecks": true,
      "target": "ES2019"
  },
  "include": [
      "./src/**/*"
  ]
}
